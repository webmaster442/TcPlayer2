<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush
        x:Key="BaseColor"
        Color="#81808E" />
    <SolidColorBrush
        x:Key="Buttons"
        Color="#DBD7CE" />
    <SolidColorBrush
        x:Key="TitleBar"
        Color="#444442" />
    <SolidColorBrush
        x:Key="TitleBarButton"
        Color="#726E6B" />
    <SolidColorBrush
        x:Key="TitleBarButtonForeground"
        Color="#17140F" />
    <SolidColorBrush
        x:Key="TitleBarText"
        Color="#A5A6AB" />
    <SolidColorBrush
        x:Key="Slider"
        Color="#FA9257" />
    <SolidColorBrush
        x:Key="Color"
        Color="#BF18ED" />
    <SolidColorBrush
        x:Key="Color2"
        Color="#66C3FA" />


    <SolidColorBrush
        x:Key="TabItem.Selected.Background"
        Color="#FFFFFF" />

    <SolidColorBrush
        x:Key="TabItem.Selected.Border"
        Color="#ACACAC" />

</ResourceDictionary>